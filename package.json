{
  "name": "forj",
  "description": "SQLite ORM and Query Builder whitout dependencies",
  "version": "0.0.3",
  "type": "module",
  "main": "src/index.ts",
  "files": ["src"],
  "exports": {
    ".": "./src/index.ts",
    "./d1": "./src/d1/index.ts",
    "./d1/types": "./src/d1/types.ts"
  },
  "-exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./d1": {
      "import": "./dist/d1.js",
      "types": "./dist/d1.d.ts"
    }
  },
  "scripts": {
    "bench": "tsx tests/benchmark/*.bench.ts",
    "build": "tsup",
    "-prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.817.0",
    "@aws-sdk/lib-dynamodb": "3.817.0",
    "pluralize": "^8.0",
    "t0n": "^0.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/pluralize": "^0.0.33",
    "@cloudflare/workers-types": "^4.20260113.0",
    "bun-types": "^1.2.13",
    "terser": "^5.46.0",
    "tiny-glob": "^0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Zunq <open-source@zunq.com>",
  "license": "MIT",
  "homepage": "https://zunq.dev",
  "repository": "git://github.com/attla/forj",
  "bugs": "https://github.com/attla/forj/issues",
  "keywords": [
    "forj",
    "attla",
    "resources",
    "support",
    "sqlite",
    "orm",
    "query",
    "builder",
    "query builder",
    "query-builder",
    "db",
    "database",
    "sql",
    "mysql",
    "postgres",
    "postgresql",
    "typescript",
    "aws",
    "lambda",
    "llrt",
    "deno",
    "bun",
    "nodejs"
  ]
}